version: '2'
services:
  chsdi3:
    build: .
    image: ${DOCKER_IMG_TAG_LATEST}
    env_file:
    - ${ENV_FILE}
    environment:
      PGUSER: ${PGUSER}
      PGPASSWORD: ${PGPASSWORD}
      OPENTRANS_API_KEY: ${OPENTRANS_API_KEY}
    command:
      - /usr/sbin/apache2ctl
      - "-D"
      - "FOREGROUND"
    ports:
      - "${APACHE_PORT}:${APACHE_PORT}"

